<section>
<% @posts.each do |post| %>
<article>
  <span class="date"> 
      <%= post.created_at.strftime("%Y-%m-%d ") %>
  </span>
  <h2>
  <%= post.title %>
  </h2>
  <p>
      <%= post.content %>
  </p>
  <div class="btn-border-bottom">
    <%=link_to("詳細","/posts/#{post.id}")%> 
  </div>
 </article>
<% end %>
</section>